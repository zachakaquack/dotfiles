alias \
\
ls='ls -C -A -p --color=auto --group-directories-first' \
reload='source $XDG_CONFIG_HOME/shell/.zshrc' \
q='exit' \
rm='rm -i' \
\
gpp='g++ *.cpp -std=c++20 -Wall && ./a.out' \
path='echo -e ${PATH//:/\\n}' \
venv='$HOME/scripts/manage_python_venv.sh' \
\
gs='git status' \
ga='git add' \
gc='git commit' \
gcm='git commit -m' \
gp='git push' \
gl='git pull' \
ginit='git init' \
